{
  "name": "avistaz/animizer",
  "description": "Anime and Manga Scrapper.",
  "keywords": [
    "anime",
    "manga",
    "database",
    "scrapper"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "6.*",
    "illuminate/support": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "symfony/var-dumper": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Animizer\\": "src/Animizer/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Animizer\\Laravel\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
